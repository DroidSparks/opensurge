// Hitboxes.
sprite SD_CAMERA_HITBOX
{
	source_file		images/camera_mask.png
	source_rect     0 0 32 16
    frame_size      16 16
    hot_spot        8 8

    // player
    animation 0
    {
        repeat      TRUE
        fps         8
        data        0
    }
	
	// camera focus
	animation 1
	{
		repeat		TRUE
		fps			1
		data		1
	}
}