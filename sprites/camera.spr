// script by Alexandre

// Camera icons
sprite SD_CAMERA
{
	source_file		images/camera.png
	source_rect     0 0 48 16
    frame_size      16 16
    hot_spot        8 8

    // x
    animation 0
    {
        repeat      TRUE
        fps         8
        data        0
    }
	
	// y
	animation 1
	{
		repeat		TRUE
		fps			8
		data		1
	}

	// cam
	animation 2
	{
		repeat		TRUE
		fps			8
		data		2
	}
}
